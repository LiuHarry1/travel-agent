{
  "mcpServers": {
    "faq": {
      "command": "python",
      "args": ["-m", "app.mcp_tools.faq.server"]
    },
    "travel-doc-retriever": {
      "command": "python",
      "args": ["-m", "app.mcp_tools.travel_doc_retriever.server"]
    },
    "tavily-mcp": {
      "command": "npx",
      "args": ["-y", "tavily-mcp@0.1.4"],
      "env": {
        "TAVILY_API_KEY": "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",
        "TAVILY_MAX_RESULTS": "5"
      }
    },
    "example-external-python": {
      "command": "python",
      "args": ["-m", "some_external_mcp_package.server"],
      "env": {
        "API_KEY": "your-api-key-here"
      }
    },
    "example-binary": {
      "command": "/path/to/mcp-server-binary",
      "args": ["--config", "/path/to/config.json"]
    }
  }
}


